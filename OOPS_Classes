// clasess in OOPS

class Student{
     String name;
     int age;
     
     void display(){
          System.out.println("Student name is : " + name);
          System.out.println("Student age is : " + age);
     }
     
}

public class OOPS{
     public static void main(String args[]){
           Student s1 = new Student();
           
           s1.name = "Chirag Arya";
           s1.age = 21;
           s1.display();
     }
}
